<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:cst="urn:import:com.binomed.cineshowtime.client.ui">
	
	<ui:style src="../resources/css/CstStyle.css" />
	<ui:with field="res" type="com.binomed.cineshowtime.client.resources.CstResource" />
	<ui:with field="i18n" type="com.binomed.cineshowtime.client.resources.I18N" />
	
	<g:VerticalPanel width="100%" >
		<cst:TheaterViewHeader ui:field="theaterHeader" />
		<g:DisclosurePanel ui:field="theaterPanel" styleName="{style.theaterDisclosure}">
			<g:SimplePanel ui:field="theaterCoverflow" styleName="{style.theaterCoverlflow}"/>
		</g:DisclosurePanel>
	</g:VerticalPanel>

</ui:UiBinder> 