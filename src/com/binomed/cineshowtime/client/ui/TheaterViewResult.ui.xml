<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:cst="urn:import:com.binomed.cineshowtime.client.ui">
	
	<ui:style src="../resources/css/CstStyle.css" />
	<ui:with field="res" type="com.binomed.cineshowtime.client.resources.CstResource" />
	<ui:with field="i18n" type="com.binomed.cineshowtime.client.resources.I18N" />
	
	<g:VerticalPanel ui:field="resultHeaderPabel" styleName="{res.css.theaterResultHeader}">
		<g:HTMLPanel>
			<div style="float:left;">
				<g:Label ui:field="firstLabel" styleName="{res.css.theaterResultLabel}"/>
			</div>
			<div style="float:right;">
				<g:Label ui:field="secondLabel" styleName="{res.css.theaterResultLabel}" />
			</div>		
		</g:HTMLPanel>
	</g:VerticalPanel>

</ui:UiBinder> 