{
  "name": "CineShowTime for Chrome",
  "version": "1.0",
  "description": "The best way to get your showtimes.",
  "background_page": "CineShowTime_Chrome_GWT.html",
  "app": {
    "launch": {
      "local_path": "CineShowTime_Chrome_GWT.html?locale=fr"
    }
  },
  "omnibox": { "keyword" : "cine" },
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "http://*.binomed-cineshowtime-chrome.appspot.com/",
    "tabs", 
    "management"
  ]
}